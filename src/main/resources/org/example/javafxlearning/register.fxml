      </Label>
      <TableView fx:id="vehicleDataTable" layoutX="241.0" layoutY="85.0" prefHeight="323.0" prefWidth="635.0">
        <columns>
          <TableColumn fx:id="nameColumn" minWidth="50.0" prefWidth="171.0" text="Name" />
            <TableColumn fx:id="typeColumn" minWidth="50.0" prefWidth="115.0" text="Type" />
            <TableColumn fx:id="modelColumn" maxWidth="1.7976931348623157E308" prefWidth="144.0" text="Model" />
            <TableColumn fx:id="licenseColumn" prefWidth="204.0" text="Licen" />
        </columns>
      </TableView>
      <TextField fx:id="nameTextField" layoutX="680.0" layoutY="50.0" />
      <Label layoutX="609.0" layoutY="55.0" text="Search" />
   </children>
</AnchorPane>
