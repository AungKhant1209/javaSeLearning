<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="481.0" prefWidth="966.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.javafxlearning.RegisterController">
   <children>
      <VBox layoutX="14.0" layoutY="109.0" prefHeight="217.0" prefWidth="196.0" spacing="20.0">
         <children>
            <Label text="Name" />
            <TextField fx:id="modelTextField" />
            <Label text="Model" />
            <TextField fx:id="licenseTextField" />
            <Label text="Type" />
            <ChoiceBox fx:id="typeTextBox" prefHeight="21.0" prefWidth="197.0" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onRegisterClicked" prefHeight="26.0" prefWidth="98.0" text="Register">
